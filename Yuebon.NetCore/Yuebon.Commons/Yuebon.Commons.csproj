<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Authors>Yuebon</Authors>
    <Company>上海越邦网络科技有限公司</Company>
    <Product>YuebonNetCore开发框架基础库</Product>
